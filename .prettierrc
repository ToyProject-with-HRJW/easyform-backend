// A code formatter that reads code and reformats it according to user options.
{
  "printWidth": 150, // 줄 바꿈 할 폭 길이
  "tabWidth": 2, // 탭 너비 
  "singleQuote": true, // single Quotations 사용 여부
  "trailingComma": "all", // 여러 줄을 사용할 때, 후행 콤마 사용 방식
  "bracketSpacing": true, // 객체 리터럴에서 괄호에 공백 삽입 여부 
  "semi": true, // 세미콜론 사용 여부
  "arrowParens": "always", // 화살표 함수 괄호 사용 방식
  "endOfLine": "auto" // 운영체제에 맞는 줄바꿈 방식 지정
}